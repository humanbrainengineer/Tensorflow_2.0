import tensorflow as tf
import os
import sys

os.environ['TF_CPP_MIN_LOG_LEVEL'] = '2'  # 不显示等级2以下的提示信息
print('GPU', tf.test.is_gpu_available())
a = tf.constant(12.0)
b = tf.constant(4.0)
print(a + b)
print(sys.version)

#Output:
# GPU True
# tf.Tensor(6.0, shape=(), dtype=float32)


